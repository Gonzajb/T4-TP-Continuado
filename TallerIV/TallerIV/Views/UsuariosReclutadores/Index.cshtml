@model IEnumerable<TallerIV.Dominio.UsuarioReclutador>

@{
    ViewBag.Title = "Reclutadores";
    Layout = "~/Views/Shared/_LayoutProui.cshtml";
}

<h2>Reclutadores</h2>

<p>
    @Html.ActionLink("Create New", "Register", "AccountReclutador")
</p>
<table class="table">
    <tr>
        <th>
            Nombre
        </th>
        <th>
            Apellido
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.Nombre
        </td>
        <td>
            @item.Apellido
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
